//consructor overloading
 ppublic class Main{
    public static void main(String[] args){
        //creating object for the class student
        Student s1 = new Student();
        Student s2 =new Student("rafi");
        Student s3 =new Student("rafi",23);

       
    }
}
class Student{
     private String name;
    private int rollNo;
 Student(){
   this.name="rafi";
        System.out.println("Name.  : " + this.name);
    }
    Student(String name){
        this.name=name;
                        System.out.println("name : " + this.name);

    }
    Student (String name,int rollNo){
        this.name=name;
        this.rollNo=rollNo;
                System.out.println("Name.  : " + this.name + " || "+"   roll no :"+ rollNo);

    }
}

//copy constructor
 public class Main{
   public class Main{
    public static void main(String[] args){
        //creating object for the class student
        Student s1 = new Student();
        Student s2 =new Student(s1);

       
    }
}
class Student{
     private String name;
    private int rollNo;
 Student(){
   this.name="rafi";
        System.out.println("Name.  : " + this.name);
    }
    Student(Student s1){
        this.name=s1.name;
       System.out.println("name : " + this.name);

    }
  

    }
 }
 //shallow copy 
 //shallow copy
class Main {
    public static void main(String[] args) {
        Student id1= new Student();
        Student id2=new Student(id1);
    }
}
class Student{
    String name;
    String address;
    int marks;
    int age ;
    Student(){
 this.name="rafi";
        this.address="venu gopala puram";
        this.marks=80;
        this.age=21;
        System.out.println("name :"+name+" "+",address :"+address+", marks : "+marks+",age : "+age);
           }
Student (Student id1){
    this.name=id1.name;
    this.address=id1.address;
    this.marks=id1.marks;
        System.out.println("name :"+name+" "+",address :"+address+", marks : "+marks+",age : "+age);
}
}
//deep copy

//shallow copy
class Main {
    public static void main(String[] args) {
        Student id1= new Student();
        Student id2=new Student(id1);
        id2.marks[2]=88;//here the marks changes using the reference of id1
        // for (int i=0;i<3;i++){
        //     System.out.println(id2.marks[i]);
        // }
        
        
        
    }
}
class Student{
    String name;
    String address;
    int marks[]=new int[3];
    int age ;
    Student(){
        this.name="rafi";
        this.address="venu gopala puram";
        this.marks[0]=85;
       this. marks[1]=89;
        this.marks[2]=82;
        this.age=21;
    }
     Student (Student id1){
        //  this.name=id1.name;
        //  this.address=id1.address;
        //  this.marks=id1.marks;
          for (int i=0;i<3;i++){
            System.out.println(id1.marks[i]);
